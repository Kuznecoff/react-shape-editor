{
  "name": "react-shape-editor",
  "description": "Simple shape editor component",
  "version": "2.6.1",
  "scripts": {
    "start": "./start.sh",
    "build": "rollup -c",
    "build:website": "npm run clean:website && parcel build website/index.html -d build --public-url /react-shape-editor/",
    "clean": "rimraf dist/*",
    "clean:website": "rimraf build",
    "lint": "eslint src website",
    "prettier": "prettier --write \"{src,example/src}/**/*.{js,css,md}\"",
    "prepublishOnly": "npm run lint && npm run test:once && npm run clean && npm run build",
    "test": "jest --watch",
    "test:once": "jest",
    "deploy": "gh-pages -d build",
    "prepush-check-prettier": "npm run prettier -- --list-different || (echo 'ERROR: Some files were updated by prettier. Please commit them and continue.'; exit 1)"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fritz-c/react-shape-editor.git"
  },
  "homepage": "https://github.com/fritz-c/react-shape-editor#readme",
  "bugs": "https://github.com/fritz-c/react-shape-editor/issues",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFiles": [
      "./test-config/shim.js"
    ],
    "setupFilesAfterEnv": [
      "./test-config/test-framework-setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-push": "npm-run-all -p prepush-check-prettier lint"
    }
  },
  "dependencies": {
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "prop-types": "^15.5.7"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@welldone-software/why-did-you-render": "^3.3.5",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.8",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-react-profiler": "^0.1.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.24.2",
    "typescript": "^3.6.3"
  },
  "keywords": [
    "react",
    "react-component"
  ]
}
